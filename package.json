{
	"name": "wy-vscode-kit",
	"displayName": "wy-vscode-kit",
	"description": "基于 Vue.js 的 VSCode 插件包。",
  "version": "1.1.0",
  "publisher": "wytxer",
	"engines": {
		"vscode": "^1.62.0",
    "node": "16"
	},
  "icon": "logo.png",
	"categories": [
		"Extension Packs"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.wyInitSettings",
				"title": "wyInitSettings"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package",
    "publish": "vsce publish"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
  },
  "extensionPack": [
    "formulahendry.auto-close-tag",
    "formulahendry.auto-rename-tag",
    "michelemelluso.code-beautifier",
    "wwm.better-align",
    "aaron-bond.better-comments",
    "ms-ceintl.vscode-language-pack-zh-hans",
    "streetsidesoftware.code-spell-checker",
    "msjsdiag.debugger-for-chrome",
    "editorconfig.editorconfig",
    "dbaeumer.vscode-eslint",
    "donjayamanne.githistory",
    "eamodio.gitlens",
    "ecmel.vscode-html-css",
    "sysoev.language-stylus",
    "thisismanta.stylus-supremacy",
    "qiu8310.minapp-vscode",
    "chenxsan.vscode-standardjs",
    "ms-vscode.vscode-typescript-tslint-plugin",
    "sodatea.velocity",
    "octref.vetur",
    "vscode-icons-team.vscode-icons",
    "tomoki1207.pdf",
    "dariofuzinato.vue-peek",
    "sdras.vue-vscode-snippets",
    "oysun.vuehelper",
    "lihuiwang.vue-alias-skip",
    "ant-design-vue.vscode-ant-design-vue-helper",
    "mikestead.dotenv"
  ],
  "license": "MIT",
  "homepage": "https://github.com/wytxer/wy-vscode-kit/#readme",
  "keywords": [
    "vscode",
    "vscode kit"
  ],
  "author": {
    "name": "wytxer",
    "url": "https://github.com/wytxer"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wytxer/wy-vscode-kit.git"
  },
  "bugs": {
    "url": "https://github.com/wytxer/wy-vscode-kit/issues"
  }
}
